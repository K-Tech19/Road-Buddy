


 <!DOCTYPE html>
 <html lang="en">
   <head>
     <%- include('../partials/_head') %>
     <link rel="stylesheet" href="styles/signup.css" />
   </head>
   <body>
     <!-- <%- include('../partials/_navbar') %> -->
     <main>
        <form action="/interestsetup/<%= currentUser.id %>?_method=PUT" method="POST">
        <% for(let i = 0; i < categories.length; i++) { %>
            <h2><%= categoryNames[i] %> </h2>
                <% for(let j = 0; j < categories[i].length; j++) { %>
                   <input type="checkbox"  name="interests" value="<%=categories[i][j] %>" name="interests"> <%= categories[i][j] %> </input>
            <% } %>
         <% } %>
        <div>
            <input type="submit">Submit New Post</input>
        </div>
        </form>
     </main>
   </body>
 </html>
 